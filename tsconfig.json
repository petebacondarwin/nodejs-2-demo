{
	"compilerOptions": {
		"target": "ES2020",
		"esModuleInterop": true,
		"module": "CommonJS",
		"lib": ["ES2020"],
		"types": ["node", "@cloudflare/workers-types"],
		"moduleResolution": "node",
		"noEmit": true,
		"skipLibCheck": true,
		"checkJs": true
	},
	"include": [
		"functions/[[path]].ts",
	]
}
